    <validationRules>
        <fullName>Annual_Gross_Booking_Required</fullName>
        <active>true</active>
        <description>Annual Gross Booking required at Stage Needs Assessment or Above</description>
        <errorConditionFormula>AND( 
BLANKVALUE(Annual_Gross_Bookings__c , 0) &lt;= 0
, Probability &gt; 0.20
, NOT($Profile.Name = &quot;API Only&quot;)
, NOT($Profile.Name = &quot;System Administrator&quot;) 
)</errorConditionFormula>
        <errorMessage>Annual Gross Bookings is required at this Stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Annual_Revenue_Required</fullName>
        <active>true</active>
        <description>Annual Revenue required at Stage Needs Assessment or Above</description>
        <errorConditionFormula>AND( 
BLANKVALUE( Annual_Revenue__c , 0) &lt;= 0,
Probability &gt; 0.20
, NOT($Profile.Name = &quot;API Only&quot;)
, NOT($Profile.Name = &quot;System Administrator&quot;) 
)</errorConditionFormula>
        <errorMessage>Annual Revenue is required at this Stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Delete_Opportunity_Products</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
 TotalOpportunityQuantity &lt; PRIORVALUE(TotalOpportunityQuantity) 
, TotalOpportunityQuantity  &gt;0
, NOT($Profile.Name = &quot;API Only&quot;) 
, NOT($Profile.Name = &quot;System Administrator&quot;) 
)</errorConditionFormula>
        <errorMessage>You cannot delete Opportunity Product, you must inactivate the Product or contact your System Administrator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Loss_Require_Reason</fullName>
        <active>true</active>
        <errorConditionFormula>AND (IsClosed , 
NOT(IsWon), 
ISPICKVAL( Loss_Reason__c ,&quot;&quot;)
, NOT($Profile.Name = &quot;API Only&quot;)
, NOT($Profile.Name = &quot;System Administrator&quot;) 
)</errorConditionFormula>
        <errorDisplayField>Loss_Reason__c</errorDisplayField>
        <errorMessage>Loss Reason is Required when selecting the Lost Deal Stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lost_Opportunity_Cannot_be_Reopened</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
ISCHANGED(StageName)
, TEXT(PRIORVALUE(StageName))=&apos;Lost&apos;
, NOT($Profile.Name = &quot;API Only&quot;)
, NOT($Profile.Name = &quot;System Administrator&quot;) 
)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>You cannot change the Stage once marked as Lost. If the Opportunity is now reestablished, please create a new Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lost_Opportunity_Cannot_be_UPDATED</fullName>
        <active>false</active>
        <errorConditionFormula>AND (
NOT(ISCHANGED(StageName))
, TEXT(PRIORVALUE(StageName))=&apos;Lost&apos;
, NOT($Profile.Name = &quot;API Only&quot;)
, NOT($Profile.Name = &quot;System Administrator&quot;) 
)</errorConditionFormula>
        <errorMessage>You cannot change an Opportunity once marked as Lost. If the Opportunity is now reestablished, please create a new Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Make_Prob_Uneditable</fullName>
        <active>false</active>
        <errorConditionFormula>OR(
ISPICKVAL(StageName, &quot;Conversion&quot;) &amp;&amp; NOT(Probability = 0.00),
ISPICKVAL(StageName, &quot;Initial Conversations&quot;) &amp;&amp; NOT(Probability = 0.10),
ISPICKVAL(StageName, &quot;Needs Assessment&quot;) &amp;&amp; NOT(Probability = 0.20),
ISPICKVAL(StageName, &quot;Value Proposition&quot;) &amp;&amp; NOT(Probability = 0.50),
ISPICKVAL(StageName, &quot;Proposal/Price Quote&quot;) &amp;&amp; NOT(Probability = 0.75),
ISPICKVAL(StageName, &quot;Proposal Submitted&quot;) &amp;&amp; NOT(Probability = 0.50),
ISPICKVAL(StageName, &quot;Legal Submitted&quot;) &amp;&amp; NOT(Probability = 0.75),
ISPICKVAL(StageName, &quot;Negotiation/Review&quot;) &amp;&amp; NOT(Probability = 0.90),
ISPICKVAL(StageName, &quot;Closed Won&quot;) &amp;&amp; NOT(Probability = 1.00),
ISPICKVAL(StageName, &quot;Closed Lost&quot;) &amp;&amp; NOT(Probability = 0.00)
)</errorConditionFormula>
        <errorMessage>Modifying the Probability on an Opportunity is not Permitted. Please follow specific Stage Names for your Opportunity</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Require_Loss_Reason_Detail</fullName>
        <active>true</active>
        <description>REQ-04926 - Update to always enforce on Lost</description>
        <errorConditionFormula>AND
(IsClosed
, NOT(IsWon)
, LEN( Loss_Reason_Detail__c )=0 
, NOT($Profile.Name = &quot;API Only&quot;)
, NOT($Profile.Name = &quot;System Administrator&quot;)
)</errorConditionFormula>
        <errorDisplayField>Loss_Reason_Detail__c</errorDisplayField>
        <errorMessage>Loss Reason Detail is Required when selecting the Lost Deal Stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Won_Opportunity_Cannot_be_Updated</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
PRIORVALUE(IsWon)
, IsWon 
, NOT($Profile.Name = &quot;API Only&quot;)
, NOT($Profile.Name = &quot;System Administrator&quot;) 
)</errorConditionFormula>
        <errorMessage>You cannot modify this Opportunity once it has been marked as Won.</errorMessage>
    </validationRules>
</CustomObject>
