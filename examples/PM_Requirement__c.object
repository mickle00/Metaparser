    <validationRules>
        <fullName>Completed_Req_Must_have_Actual_Hours</fullName>
        <active>true</active>
        <description>Completed Requirement must have Actual Hours assigned</description>
        <errorConditionFormula>AND (
ISPICKVAL(Status__c , &apos;Completed&apos;) ,
Actual_Hours__c =0
)</errorConditionFormula>
        <errorMessage>You must indicate Actual Hours completed in order to Complete this Requirement</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Release_Must_Be_Required_Requirement</fullName>
        <active>true</active>
        <description>Make Release required to &quot;Verified for Deployment or Completed&quot; a Requirement</description>
        <errorConditionFormula>AND (
OR (
        ISPICKVAL(Status__c , &apos;Verified for Deployment&apos;) ,
        ISPICKVAL(Status__c , &apos;Completed&apos;)
       ),
Release__c = null
)</errorConditionFormula>
        <errorDisplayField>Release__c</errorDisplayField>
        <errorMessage>Release required to &quot;Verified for Deployment or Completed&quot; a Requirement !</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requirement_Must_have_an_Assigned_To</fullName>
        <active>true</active>
        <description>Requirement must have an assigned to when open.</description>
        <errorConditionFormula>AND (
AND(
NOT( ISPICKVAL( Status__c , &apos;New&apos;) ),
NOT( ISPICKVAL( Status__c , &apos;Cancelled&apos;) ),
NOT( ISPICKVAL( Status__c , &apos;Not a Requirement&apos;) ),
NOT( ISPICKVAL( Status__c , &apos;Duplicate&apos;) ),
NOT( ISPICKVAL( Status__c , &apos;On Hold&apos;) )
),
 ISBLANK( Assigned_To__c ) 
)</errorConditionFormula>
        <errorDisplayField>Assigned_To__c</errorDisplayField>
        <errorMessage>You must select an Assigned To in this Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requirement_Must_have_an_Impacted_Team</fullName>
        <active>true</active>
        <description>Requirement must have at least one Impacted Team identified when open.</description>
        <errorConditionFormula>AND (
AND(
NOT( ISPICKVAL( Status__c , &apos;New&apos;) ),
NOT( ISPICKVAL( Status__c , &apos;Cancelled&apos;) ),
NOT( ISPICKVAL( Status__c , &apos;Not a Requirement&apos;) ),
NOT( ISPICKVAL( Status__c , &apos;Duplicate&apos;) ),
NOT( ISPICKVAL( Status__c , &apos;On Hold&apos;) )
),
 Impacted_Team_Count__c = 0
)</errorConditionFormula>
        <errorMessage>You must select an Impacted Team</errorMessage>
    </validationRules>
